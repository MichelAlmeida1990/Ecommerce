{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "prefer-const": "warn",
    "no-var": "error",
    "no-console": "warn",
    "eqeqeq": "warn",
    "curly": "warn",
    "no-multiple-empty-lines": ["warn", { "max": 2 }],
    "no-trailing-spaces": "warn",
    "eol-last": "warn"
  }
}




